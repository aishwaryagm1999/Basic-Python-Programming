str1 = str(input()) # string input
k = int(input()) #number of times string is rotated to the left
ls = []

for i in range(len(str1)): #code for converting string to list
    ls.append(str1[i])
    
ls1 = len(ls)
for i in range(k): #code for rotation of string to the left
    
    ls.insert((ls1),ls[0])
    del ls[0]
    
finalstr = "" #code to convert list to string
for i in range(0,len(ls)):
      finalstr = finalstr+ls[i]
print(finalstr)
